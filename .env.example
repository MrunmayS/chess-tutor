# OpenAI API Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4o

# vLLora Gateway Configuration
# Set USE_LOCAL_GATEWAY=true to route requests through vLLora for tracing
USE_LOCAL_GATEWAY=false
LLM_BASE_URL=http://localhost:9090/v1

# Stockfish Configuration (optional - will auto-detect if on PATH)
STOCKFISH_PATH=/usr/bin/stockfish

# Analysis Settings
STOCKFISH_DEPTH=15
STOCKFISH_MULTIPV=3

# Logging
LOG_LEVEL=INFO
